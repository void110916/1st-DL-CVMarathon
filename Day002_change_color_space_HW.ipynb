   "source": [
    "import cv2\n",
    "img1 = cv2.imread('lena.png', cv2.IMREAD_COLOR)\n",
    "cv2.imshow('BGR', img1)\n",
    "\n",
    "img2=cv2.cvtColor(img1, cv2.COLOR_BGR2HLS)\n",
    "cv2.imshow('HLS', img2)\n",
    "\n",
    "img3=cv2.cvtColor(img1, cv2.COLOR_BGR2LAB)\n",
    "cv2.imshow('LAB', img3)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
